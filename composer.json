{
  "name": "tw2113/michaelbox",
  "description": "My plugins",
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/classic-editor": "1.6.2",
    "wpackagist-plugin/disable-emojis": "1.7.4",
    "wpackagist-plugin/ninja-forms": "3.6.14",
    "wpackagist-plugin/query-monitor": "3.10.1",
    "wpackagist-plugin/sucuri-scanner": "1.8.36",
    "wpackagist-plugin/wa11y": "1.0.3",
    "wpackagist-plugin/wp-to-twitter": "3.6.2",
    "wpackagist-plugin/wp-piwik": "1.0.27",
    "wpackagist-theme/blogsixteen": "1.4.6",
    "wpackagist-plugin/better-wp-security": "8.1.4",
    "wpackagist-plugin/cmb2": "2.10.1",
    "wpackagist-plugin/webmention": "4.0.8",
    "wpackagist-plugin/semantic-linkbacks": "3.11.1",
    "wpackagist-plugin/wp-uf2": "1.1.0",
    "wpackagist-plugin/publishpress": "3.10.1",
    "wpackagist-plugin/untappd-checkins-widget": "1.3.3",
    "wpackagist-plugin/public-post-preview": "2.10.0",
    "wpackagist-plugin/autodescription": "4.2.7.1",
    "wpackagist-plugin/conditional-widgets": "3.1",
    "wpackagist-plugin/reading-time-wp": "2.0.12",
    "wpackagist-plugin/breadcrumb-navxt": "7.1.0",
    "wpackagist-plugin/blogroll-block": "1.3.1",
    "wpackagist-plugin/ewww-image-optimizer": "6.9.3",
    "wpackagist-plugin/table-of-contents": "1.0.2",
    "wpackagist-plugin/antispam-bee": "2.11.1",
    "wpackagist-plugin/social-media-shortcodes": "1.2.0"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
