{
  "name": "tw2113/michaelbox",
  "description": "My plugins",
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/antispam-bee": "2.11.3",
    "wpackagist-plugin/autodescription": "4.2.8",
    "wpackagist-plugin/avatar-privacy": "2.7.0",
    "wpackagist-plugin/better-wp-security": "8.1.8",
    "wpackagist-plugin/blogroll-block": "1.3.3",
    "wpackagist-plugin/breadcrumb-navxt": "7.2.0",
    "wpackagist-plugin/classic-editor": "1.6.3",
    "wpackagist-plugin/cmb2": "2.10.1",
    "wpackagist-plugin/conditional-widgets": "3.1",
    "wpackagist-plugin/disable-emojis": "1.7.5",
    "wpackagist-plugin/ewww-image-optimizer": "7.2.0",
    "wpackagist-plugin/ninja-forms": "3.6.29",
    "wpackagist-plugin/public-post-preview": "2.10.0",
    "wpackagist-plugin/publishpress": "3.12.0",
    "wpackagist-plugin/query-monitor": "3.13.1",
    "wpackagist-plugin/reading-time-wp": "2.0.13",
    "wpackagist-plugin/semantic-linkbacks": "3.12.0",
    "wpackagist-plugin/social-media-shortcodes": "1.2.1",
    "wpackagist-plugin/sucuri-scanner": "1.8.39",
    "wpackagist-plugin/table-of-contents": "1.0.2",
    "wpackagist-plugin/untappd-checkins-widget": "1.3.3",
    "wpackagist-plugin/wa11y": "1.0.3",
    "wpackagist-plugin/webmention": "5.1.3",
    "wpackagist-plugin/wp-piwik": "1.0.28",
    "wpackagist-plugin/wp-uf2": "1.1.0",
    "wpackagist-theme/blogsixteen": "1.4.6"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    },
    "sort-packages": true
  }
}
