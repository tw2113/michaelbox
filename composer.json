{
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/classic-editor": "1.6",
    "wpackagist-plugin/disable-emojis": "1.7.2",
    "wpackagist-plugin/ninja-forms": "3.4.34",
    "wpackagist-plugin/query-monitor": "3.6.7",
    "wpackagist-plugin/sucuri-scanner": "1.8.25",
    "wpackagist-plugin/wa11y": "1.0.3",
    "wpackagist-plugin/zero-spam": "4.10.2",
    "wpackagist-plugin/wp-to-twitter": "3.5.0",
    "wpackagist-plugin/wp-piwik": "1.0.24",
    "wpackagist-theme/blogsixteen": "1.4.6",
    "wpackagist-plugin/better-wp-security": "7.9.0",
    "wpackagist-plugin/cmb2": "2.7.0",
    "wpackagist-plugin/webmention": "4.0.5",
    "wpackagist-plugin/semantic-linkbacks": "3.10.4",
    "wpackagist-plugin/wp-uf2": "1.1.0",
    "wpackagist-plugin/indieweb-post-kinds": "3.4.17",
    "wpackagist-plugin/publishpress": "3.1.0",
    "wpackagist-plugin/untappd-checkins-widget": "1.3.3",
    "wpackagist-plugin/public-post-preview": "2.9.2",
    "wpackagist-plugin/newsletter-glue": "1.1.13",
    "wpackagist-plugin/autodescription": "4.1.3",
    "wpackagist-plugin/conditional-widgets": "2.2"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  }
}
