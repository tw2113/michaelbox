{
  "name": "tw2113/michaelbox",
  "description": "My plugins",
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/antispam-bee": "2.11.6",
    "wpackagist-plugin/autodescription": "5.0.6",
    "wpackagist-plugin/avatar-privacy": "2.7.0",
    "wpackagist-plugin/better-wp-security": "9.3.3",
    "wpackagist-plugin/blogroll-block": "1.3.5",
    "wpackagist-plugin/breadcrumb-navxt": "7.3.1",
    "wpackagist-plugin/classic-editor": "1.6.5",
    "wpackagist-plugin/cmb2": "2.11.0",
    "wpackagist-plugin/conditional-widgets": "3.2",
    "wpackagist-plugin/disable-emojis": "1.7.6",
    "wpackagist-plugin/ewww-image-optimizer": "7.9.0",
    "wpackagist-plugin/ninja-forms": "3.8.17",
    "wpackagist-plugin/public-post-preview": "2.10.0",
    "wpackagist-plugin/publishpress": "4.5.0",
    "wpackagist-plugin/query-monitor": "3.16.4",
    "wpackagist-plugin/reading-time-wp": "2.0.16",
    "wpackagist-plugin/redirection": "5.5.0",
    "wpackagist-plugin/semantic-linkbacks": "3.12.0",
    "wpackagist-plugin/social-media-shortcodes": "1.3.0",
    "wpackagist-plugin/sucuri-scanner": "1.9.5",
    "wpackagist-plugin/table-of-contents": "1.0.2",
    "wpackagist-plugin/untappd-checkins-widget": "1.3.3",
    "wpackagist-plugin/wa11y": "1.0.3",
    "wpackagist-plugin/webmention": "5.3.3",
    "wpackagist-plugin/wp-piwik": "1.0.30",
    "wpackagist-plugin/wp-uf2": "1.1.0",
    "wpackagist-theme/blogsixteen": "1.4.6"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    },
    "sort-packages": true
  }
}
